{
  "name": "@equito-sdk/core",
  "version": "0.0.1",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "repository": {
    "directory": "packages/core",
    "type": "git",
    "url": "https://github.com/equito-network/equito-sdk.git"
  },
  "type": "module",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "pnpm clean && tsup src/index.ts --format cjs,esm --dts",
    "lint": "tsc --noEmit",
    "prepack": "pnpm build",
    "docs": "typedoc src/index.ts --options typedoc.config.json --tsconfig tsconfig.json"
  },
  "devDependencies": {
    "@equito-sdk/config": "workspace:*",
    "tsup": "^8.1.0",
    "typedoc": "^0.26.3",
    "typescript": "^5.5.2"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
