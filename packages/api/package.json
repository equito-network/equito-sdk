{
  "name": "@equito-sdk/api",
  "version": "0.0.1",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "repository": {
    "directory": "packages/api",
    "type": "git",
    "url": "https://github.com/equito-network/equito-sdk.git"
  },
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
