      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "lint": "tsc"
  },
  "devDependencies": {
    "@equito-sdk/config": "workspace:*",
    "tsup": "^8.1.0",
    "typescript": "^5.5.2"
  },
  "dependencies": {
    "@equito-sdk/core": "workspace:*",
    "@polkadot/api": "^12.0.1"
  },
  "files": [
    "dist",
    "README.md"
  ]
